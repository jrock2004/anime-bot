{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2017"
    ],
    "module": "commonjs",
    "noImplicitAny": true,
    "rootDir": "src",
    "outDir": "functions",
    "target": "es5",
    "esModuleInterop": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
