{
  "name": "anime-bot",
  "version": "1.0.0",
  "description": "A bot for Slack and Mattermost to look up anime",
  "main": "functions/server.js",
  "scripts": {
    "start": "NODE_ENV=dev TOKEN=9999 netlify-lambda serve functions",
    "build": "netlify-lambda build functions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrock2004/anime-bot.git"
  },
  "keywords": [
    "anime",
    "slack",
    "mattermost"
  ],
  "author": "jrock2004",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jrock2004/anime-bot/issues"
  },
  "homepage": "https://github.com/jrock2004/anime-bot#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.27.0",
    "serverless-http": "^2.5.0",
    "string-strip-html": "^4.5.0",
    "turndown": "^6.0.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-plugin-node": "^11.1.0",
    "netlify-lambda": "^2.0.0"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "engines": {
    "node": ">13.0.0"
  }
}
